pipeline {
   agent any
     stages {
         stage('verify') {
            steps { 
                    sh " docker --version"
              }
         }
         stage('run') {
          steps {
                    sh " docker-compose up -d"
              }
          }
          stage('build'){
             steps { 
                  sh " docker-compose build"
               }
           }
        }
     }   
